{
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc",
    "fmt": "prettier --loglevel=warn --write \"src/**/*.ts\"",
    "check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" -f mo",
    "lint:fix": "eslint \"src/**/*.ts\" --fix -f mo",
    "start": "node --no-warnings --experimental-specifier-resolution=node dist"
  },
  "dependencies": {
    "discord.js": "13.8.0",
    "dotenv": "^16.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-mo": "^1.2.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.2"
  }
}
